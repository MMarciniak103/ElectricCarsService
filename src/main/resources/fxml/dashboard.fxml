<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.lynden.gmapsfx.ClusteredGoogleMapView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import com.lynden.gmapsfx.GoogleMapView?>
<AnchorPane fx:id="dashboardPane" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #212121;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.DashboardController">
   <children>
       <JFXButton fx:id="adminBtn" layoutX="699.0" layoutY="14.0" onAction="#openAdminPanel" style="-fx-background-radius: 90;" text="Admin Panel" textFill="#757575">
         <font>
            <Font name="Tahoma Bold" size="13.0" />
         </font>
         <graphic>
            <ImageView fitHeight="27.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icons/settings.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>

      <JFXButton fx:id="logoutBtn" layoutX="14.0" layoutY="14.0" onAction="#logout" text="Logout" textFill="#757575">
         <graphic>
            <ImageView fitHeight="22.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icons/logout.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <TableView fx:id="carTable" layoutX="14.0" layoutY="400.0" prefHeight="97.0" prefWidth="800.0" stylesheets="@../css/tableView.css">
        <columns>
          <TableColumn prefWidth="166.0" text="Name" />
          <TableColumn prefWidth="123.0" text="Battery Lvl Pct" />
            <TableColumn prefWidth="145.0" text="Address" />
            <TableColumn prefWidth="74.0" text="Status" />
            <TableColumn prefWidth="152.0" text="Battery Distance" />
            <TableColumn prefWidth="139.0" text="Damage Description" />
        </columns>
      </TableView>
      <JFXButton fx:id="rentBtn" layoutX="94.0" layoutY="497.0" onAction="#rentCar" prefHeight="51.0" prefWidth="160.0" style="-fx-background-color: #76FF03; -fx-background-radius: 90;" text="Rent" textFill="#757575">
         <font>
            <Font name="Tahoma Bold" size="22.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="returnBtn" layoutX="619.0" layoutY="497.0" onAction="#returnCar" prefHeight="51.0" prefWidth="160.0" style="-fx-background-color: #76FF03; -fx-background-radius: 90;" text="Return" textFill="#757575">
         <font>
            <Font name="Tahoma Bold" size="22.0" />
         </font>
      </JFXButton>
      <GoogleMapView fx:id="mapViewer" layoutX="14.0" layoutY="51.0" prefHeight="342.0" prefWidth="800.0" />
   </children>
</AnchorPane>
